{
    "name": "amaz/laravel-unique-slug",
    "description": "This is Simple slug generator",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Amaz\\LaravelSlug\\": "src/"
        }
    },
    "keywords": [
        "laravel_slug_generator",
        "laravel_unique_slug",
        "slug_generator",
        "php Slug",
        "laravel"
    ],
    "homepage": "https://github.com/amazbhola/sulg-generator-laravel.git",
    "authors": [
        {
            "name": "amazbhola",
            "email": "amazingbhola@gmail.com"
        }
    ],
    "repositories": [
        {
          "type": "path",
          "url": "./laravel-slug-generator"
        }
      ],
      "extra": {
        "laravel": {
            "providers": [
                "Amaz\\LaravelSlug\\UniqueSlugServiceProvider"
            ],
            "aliases": {
                "UniqueSlug": "Barryvdh\\Debugbar\\UniqueSlug"
            }
        }
    },

    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4"
    }
}
